# Regex to expand multichar symbols (in the fst sense) to a string of individual letters.
# Will make sure that various tools do not choke on parsing input strings.

"D́" -> {D́},
"T́" -> {T́},
"B́" -> {B́},
"F́" -> {F́},
"H́" -> {H́},
"V́" -> {V́},
"d́" -> {d́},
"t́" -> {t́},
"b́" -> {b́},
"f́" -> {f́},
"h́" -> {h́},
"v́" -> {v́};
